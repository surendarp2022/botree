<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
        
<bean id="b1" class="Myspel.Book1"> 
<property name="book1id" value="2000"></property>
<property name="book1name" value="c++"></property>

</bean>    
<bean id="b2" class="Myspel.Book2"> 
<property name="book2id" value="#{T(java.lang.Math).subtractExact(6,5)}"></property>
<property name="book2name" value="java"></property>

</bean>     
        
 </beans>